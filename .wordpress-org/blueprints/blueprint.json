      },
      "options": {
        "activate": true
      }
    },
    {
      "step": "login",
      "username": "admin",
      "password": "password"
    },
    {
      "step": "runPHP",
      "code": "<?php include 'wordpress/wp-load.php'; wp_insert_term( 'Apple',  'category'); wp_insert_term( 'Orange',  'category'); wp_insert_term( 'Pineapple',  'category');"
    }
  ]
}